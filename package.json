{
  "name": "jasql",
  "version": "0.4.3",
  "description": "JSON document storage in SQL",
  "main": "lib/index.js",
  "scripts": {
    "lint": "standard \"src/*.js\" \"test/*.js\" | snazzy",
    "_test": "tape -r babel-core/register \"test/test*.js\" | tap-diff",
    "test": "npm run lint && NODE_ENV=test nyc npm run _test",
    "dev": "nodemon -w src -w test -x npm test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "babel src --out-dir lib",
    "prepublish": "npm run build"
  },
  "babel": {
    "presets": [
      "es2016-node4"
    ],
    "plugins": [
      "add-module-exports",
      "transform-async-to-generator"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "src/*.js"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codyzu/jasql.git"
  },
  "keywords": [
    "json",
    "mysql",
    "postgres",
    "postgresql",
    "sql",
    "sqlite",
    "database"
  ],
  "author": "codyzu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codyzu/jasql/issues"
  },
  "homepage": "https://codyzu.github.io/jasql",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "^2.0.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2016-node4": "^6.0.1",
    "babel-register": "^6.9.0",
    "blue-tape": "^1.0.0",
    "coveralls": "^2.11.9",
    "delete": "^0.3.2",
    "faucet": "0.0.1",
    "mkdirp-promise": "^3.0.1",
    "nodemon": "^1.9.2",
    "nyc": "^8.1.0",
    "snazzy": "^5.0.0",
    "standard": "^9.0.2",
    "tap-diff": "^0.1.1",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "knex": "^0.12.0",
    "lodash": "^4.13.1",
    "shortid": "^2.2.6"
  },
  "optionalDependencies": {
    "mysql": "^2.11.1",
    "pg": "^6.0.1",
    "sqlite3": "^3.1.4"
  }
}
