{
  "name": "jasql",
  "version": "0.1.0",
  "description": "JSON document storage in SQL",
  "main": "index.js",
  "scripts": {
    "lint": "standard | snazzy",
    "_test": "tape -r babel-register \"test/test*.js\" | tap-diff",
    "test": "npm run lint && nyc --require babel-core/register npm run _test",
    "dev": "nodemon -w lib -w test -x npm test"
  },
  "babel": {
    "presets": [
      "es2016-node5"
    ],
    "plugins": [
      "transform-async-to-generator"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codyzu/jasql.git"
  },
  "keywords": [
    "json",
    "sql"
  ],
  "author": "codyzu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codyzu/jasql/issues"
  },
  "homepage": "https://github.com/codyzu/jasql#readme",
  "devDependencies": {
    "blue-tape": "^0.2.0",
    "nodemon": "^1.9.2",
    "nyc": "^7.0.0",
    "snazzy": "^4.0.0",
    "standard": "^7.1.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "dependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.1",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-preset-es2016-node5": "^1.1.2",
    "babel-register": "^6.9.0",
    "faucet": "0.0.1",
    "lodash": "^4.13.1",
    "shortid": "^2.2.6",
    "sqlite3": "^3.1.4",
    "tap-diff": "^0.1.1"
  }
}
